@model List<GameCatalog.Models.Developer>

<h1>Developers in Database</h1>

@if (Model.Count == 0)
{
    <p>Your developers are in another castle!</p>
}
else
{
    @foreach (Developer developer in Model)
    {
        <li>
            <a asp-controller="Developer" asp-action="About" asp-route-id="@developer.Id">@developer.Name</a>
        </li>
    }
}